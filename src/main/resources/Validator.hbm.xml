<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.anemoi.itr.validation.Validator" table="validator_config">


		<id name="command" type="java.lang.String">             
			<column name="command" />             
        </id> 
		<property name="datatype">	<column name="datatype" />	</property>
		<property name="execution">	<column name="execution" />	</property>
		<property name="formula">	<column name="formula" />	</property>
		<property name="desc"> 		<column name="description" />	</property>
				
	</class>
	</hibernate-mapping>
	